pipeline {
    agent any
    tools{
        maven 'maven-3'
    }

    stages {
        stage('Git Checkout') {
            steps {
               git url:'https://github.com/bcreddydevops/mavenweb-demo.git', branch: 'main'
            }
        }
        stage('Maven Build') {
            steps {
               sh 'mvn clean package'
            }
        }
    }
}
